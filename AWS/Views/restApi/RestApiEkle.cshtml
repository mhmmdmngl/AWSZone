@model AWS.Models.restapiModel

@{
    ViewBag.Title = "RestApiEkle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
        <div class="card card-primary">
            <div class="card-header">
                <h3 class="card-title">Rest Api Ekle</h3>
            </div>
            <!-- /.card-header -->
            <!-- form start -->
            <form action="restApiEkle" method="post">
                <div class="card-body">
                    <div class="form-group">
                        <label for="exampleInputEmail1">Adı</label>
                        <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Rest Api Url." name="txtrest">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Method</label>
                        <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Rest Api Metot." name="txtmetot">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Method</label>
                        <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Servis Adı" name="txtRestApi">
                    </div>

                </div>
                <!-- /.card-body -->

                <div class="card-footer">
                    <button type="submit" class="btn btn-success float-right">Rest Api Ekle</button>
                </div>
            </form>
        </div>
    </div>
    <div class="col-md-2"></div>


</div>


